#!/bin/bash
#SBATCH --job-name="hello"
#SBATCH --output="hello-%j.out"
#SBATCH --partition=trial
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=8
#SBATCH --mem=4G
#SBATCH --account=RFST
#SBATCH --export=ALL
#SBATCH -t 00:00:15

echo "Starting Hello World with 8 threads" 
export OMP_NUM_THREADS=8
./hello_omp.exe
